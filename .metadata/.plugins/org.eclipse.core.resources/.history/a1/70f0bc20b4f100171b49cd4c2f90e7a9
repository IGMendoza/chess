package chess;

import java.io.IOException;
import java.util.LinkedList;

public class Main {
	public static void main(String[] args) throws IOException {
		System.out.println("ayy lmao");
		Board board = new Board();
		LinkedList<LinkedList<String>> coords = board.getBoard();
		for(int i = 0; i < 8; i++) {
			for(int j = 0; j < 8; j++) {
				System.out.print(coords.get(i).get(j));
				System.out.print(" ");
			}
			System.out.println("");
		}
	}
}
